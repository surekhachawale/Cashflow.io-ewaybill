<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>
    <SelectDialog
        title="Plant"
        growingThreshold="20"
        search="_onplantSearch"
        confirm="_onplantValueHelpConfirm"
        items="{plantModel>/results}"
        multiSelect="true"
    >
        <customData>
            <core:CustomData
                key="selectedPropertyPath"
                value="{plantModel>Plant}"
                writeToDom="false"
            />
        </customData>
        <items>
            <StandardListItem
                title="{plantModel>Plant}"
                description="{plantModel>PlantName}"
                infoState="None"
            />
        </items>
    </SelectDialog>
</core:FragmentDefinition>
